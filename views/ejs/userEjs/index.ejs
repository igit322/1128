    <meta property="og:image" content="/public/images/resoft.png"/>
    <title>Resoft</title>
	<!-- favicon -->
    <link rel="shortcut icon" href="/public/images/img1/favicon.png" type="image/x-icon">

    <link rel="stylesheet" href="/public/css/reset.css">
    <link rel="stylesheet" href="/public/css/animate.css">
    <link rel="stylesheet" href="/public/css/magic.css">

    <link rel="stylesheet" href="/public/css/Main/main.css">
